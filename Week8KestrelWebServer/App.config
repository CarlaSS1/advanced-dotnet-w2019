<configuration>
  <connectionStrings>
    <add name="Week8DbConnection" connectionString="Data Source=.;Initial Catalog=Week8Db;Integrated Security=SSPI;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <environment value="Development" />
  <logging path="week_8_kestrel_web_server.log">
    <switches>
      <switch name="Week8KestrelWebServer">Trace</switch>
    </switches>
  </logging>
  <kestrel>
    <port>12040</port>
  </kestrel>
</configuration>